  },
  "team": {
    "maintainers": ["CSR开发团队"],
    "reviewers": ["孙雄鹰", "张如诚", "xu jin"]
  },
  "publish": {
    "enabled": true,
    "public": false,
    "domain": "csr-admin-docs"
  }
}
