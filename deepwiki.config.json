  "version": "1.0.0",
  "docs": {
    "input": ["API_DOCS.md", "src/**/*.js", "src/**/*.jsx"],
    "output": "docs",
    "autoUpdate": true,
    "generateWeb": true
  },
  "api": {
    "baseUrl": "/api",
